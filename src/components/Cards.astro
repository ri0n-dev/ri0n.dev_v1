<style>
  @keyframes marquee {
    0% {
      transform: translateX(0);
      opacity: 1;
    }
    100% {
      transform: translateX(-50%);
      opacity: 0;
    }
  }

  .marquee {
    position: relative;
    overflow: hidden;
  }

  .marquee::before {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: linear-gradient(
      to right,
      #ffffff 0%,
      transparent 50%,
      #ffffff 100%
    );
    pointer-events: none;
    z-index: 1;
  }

  @keyframes marquee {
    0% {
      transform: translateX(0);
    }
    100% {
      transform: translateX(-50%);
    }
  }

  .animate-marquee {
    animation: marquee 20s linear infinite;
    display: inline-block;
    white-space: nowrap;
    position: relative;
    z-index: 0;
  }

  .animate-marquee svg {
    width: 30px;
    display: inline-block;
    margin-right: 30px;
    fill: #1f1f1f;
  }

  .skeleton {
    background: linear-gradient(90deg, #eee 25%, #ddd 50%, #eee 75%);
    background-size: 200% 100%;
    animation: shimmer 5s infinite linear;
    border-radius: 5px;
  }

  @keyframes shimmer {
    0% {
      background-position: -200% 0;
    }
    100% {
      background-position: 200% 0;
    }
  }
</style>
